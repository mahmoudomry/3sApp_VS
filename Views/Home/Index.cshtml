    <script>
        $(document).ready(function () {
            function padWithZero(num, targetLength) {
                return String(num).padStart(targetLength, '0');
            }
            /* Client */
            var mainSlider = $('#mainSliderCarousel').owlCarousel({
                slideSpeed: 300,
                dots: true,

                items: 1,
                itemsDesktop: false,
                itemsDesktopSmall: false,
                itemsTablet: false,
                itemsMobile: false
            });
            mainSlider.on('changed.owl.carousel', function (event) {
                var num = event.item.index + 1;
                var count = event.item.count;
                $('#main-slider-style').text('.main-slider .owl-dots::before{content:"' + padWithZero(num, 2) + '";}.main-slider .owl-dots::after{content:"' + padWithZero(count, 2) + '";}');
            });

            $('.partners-carousel').owlCarousel({
                loop: true,
                margin: 20,
                responsiveClass: true,
                nav: true,
                navText: ['<i class="fa-sharp fa-solid fa-arrow-left"></i>', '<i class="fa-sharp fa-solid fa-arrow-right"></i>'],
                responsive: {
                    0: {
                        items: 3,
                        nav: true
                    },
                    600: {
                        items: 3,
                        nav: false
                    },
                    1000: {
                        items: 6,
                        nav: true,
                        loop: false
                    }
                }
            });
            $('.clients-carousel').owlCarousel({
